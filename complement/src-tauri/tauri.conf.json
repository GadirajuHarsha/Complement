{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "complement",
  "version": "0.1.0",
  "identifier": "com.harsh.complement",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "height": 80,
        "resizable": false,
        "title": "Complement",
        "width": 715,
        "decorations": false,
        "transparent": true,
        "center": true,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}